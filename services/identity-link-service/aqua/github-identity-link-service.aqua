import "@fluencelabs/aqua-lib/builtin.aqua"

data RequesterPeer:
  peerId: PeerId
  relayPeerId: PeerId

data GithubRequest:
  did: string
  username: string

service GithubIdentityLinkService("github-identity-link-service"):
  githubRequest(req: GithubRequest, reqPeer: RequesterPeer)
  githubVerify(req: GithubRequest, reqPeer: RequesterPeer)
